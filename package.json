{
  "name": "osu-schedule-board",
  "version": "1.0.0",
  "description": "A schedule board for managing aircraft operations",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all --parallel express-server watch:sass",
    "express-server": "nodemon app.js",
    "build": "webpack -p",
    "watch": "webpack --watch",
    "watch:sass": "node-sass css/sass/main.scss css/style.css -w",
    "compile:sass": "node-sass css/sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Jory Schmidt",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "client-sessions": "^0.8.0",
    "express": "^4.15.3",
    "md5": "^2.2.1",
    "mongoose": "^4.10.4",
    "morgan": "^1.8.2",
    "node-sass-middleware": "^0.11.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "sinon": "^7.1.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  }
}
